<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xi="http://www.w3.org/2001/XInclude"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <managed-bean>
        <managed-bean-name>helloController</managed-bean-name>
        <managed-bean-class>HelloController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>pointController</managed-bean-name>
        <managed-bean-class>PointController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <validator>
        <validator-id>ValidatorR</validator-id>
        <validator-class>ValidatorR</validator-class>
    </validator>

    <managed-bean>
        <managed-bean-name>helloBean</managed-bean-name>
        <managed-bean-class>HelloBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>userInfo</managed-bean-name>
        <managed-bean-class>UserInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>DBConnector</managed-bean-name>
        <managed-bean-class>DBConnector</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>


    <managed-bean>
        <managed-bean-name>spinner</managed-bean-name>
        <managed-bean-class>MySpinner</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>


    <managed-bean>
        <managed-bean-name>now</managed-bean-name>
        <managed-bean-class>java.util.Date</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>



</faces-config>
